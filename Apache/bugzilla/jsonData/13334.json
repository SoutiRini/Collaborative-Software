[{"count": 0, "tags": [], "creator": "vladimir@bossicard.com", "attachment_id": null, "text": "Goal is to integrate Commons-logging", "id": 24002, "time": "2002-10-05T19:53:33Z", "bug_id": 13334, "creation_time": "2002-10-05T19:53:33Z", "is_private": false}, {"count": 1, "tags": [], "bug_id": 13334, "text": "Created attachment 3363\nintegration of Commons-logging", "id": 24003, "time": "2002-10-05T19:54:56Z", "creator": "vladimir@bossicard.com", "creation_time": "2002-10-05T19:54:56Z", "is_private": false, "attachment_id": 3363}, {"text": "To use this pacth with Tomcat, you have to modify the bin/catalina.sh (.bat) to\ninclude the following line:\n\nCATALINA_OPTS=\"-Dorg.apache.commons.logging.simplelog.defaultlog=trace\"\n\nIt's not very elegent but it works.\n", "tags": [], "creator": "vladimir@bossicard.com", "attachment_id": null, "count": 2, "id": 24004, "time": "2002-10-05T19:58:00Z", "bug_id": 13334, "creation_time": "2002-10-05T19:58:00Z", "is_private": false}, {"count": 3, "tags": [], "bug_id": 13334, "text": "The XindiceServlet still has some test calls.  Please remove them", "id": 24005, "time": "2002-10-05T20:43:01Z", "creator": "vladimir@bossicard.com", "creation_time": "2002-10-05T20:43:01Z", "is_private": false, "attachment_id": null}]