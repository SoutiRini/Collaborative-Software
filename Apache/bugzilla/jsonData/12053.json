[{"count": 0, "tags": [], "bug_id": 12053, "text": "The manager cannot reset/start/stop the root context", "id": 21755, "time": "2002-08-26T20:45:14Z", "creator": "chadj@netnet.net", "creation_time": "2002-08-26T20:45:14Z", "is_private": false, "attachment_id": null}, {"count": 1, "tags": [], "bug_id": 12053, "attachment_id": null, "text": "I initially submitted this bug incorrectly. Should read:\n\nThe html manager cannot reset/start/stop the root context\n", "id": 22637, "time": "2002-09-11T01:54:14Z", "creator": "chadj@netnet.net", "creation_time": "2002-09-11T01:54:14Z", "is_private": false}, {"attachment_id": 3006, "tags": [], "bug_id": 12053, "is_private": false, "count": 2, "id": 22642, "time": "2002-09-11T02:51:44Z", "creator": "chadj@netnet.net", "creation_time": "2002-09-11T02:51:44Z", "text": "Created attachment 3006\nProposed patch for   org.apache.catalina.servlets.HTMLManagerServlet"}, {"count": 3, "tags": [], "bug_id": 12053, "text": "The displayPath for the root context was getting renamed to \"/\" to soon.  \ndeployer.findDeployedApp expects the root context to be referred to as \"\" and \nnot \"/\".  The patch above fixes this problem.", "id": 22643, "time": "2002-09-11T02:56:11Z", "creator": "chadj@netnet.net", "creation_time": "2002-09-11T02:56:11Z", "is_private": false, "attachment_id": null}, {"count": 4, "tags": [], "creator": "remm@apache.org", "attachment_id": null, "text": "The fix will be in 4.1.11.", "id": 23035, "time": "2002-09-18T14:15:46Z", "bug_id": 12053, "creation_time": "2002-09-18T14:15:46Z", "is_private": false}]