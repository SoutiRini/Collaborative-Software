[{"count": 0, "tags": [], "creator": "social@v3.sk", "attachment_id": 31251, "is_private": false, "id": 172676, "time": "2014-01-24T15:47:02Z", "bug_id": 56062, "creation_time": "2014-01-24T15:47:02Z", "text": "Created attachment 31251\npatch to allow decompression of files larger than 4GB\n\nUpload of compressed file that is originally larger than 4GB ends up with \"Could not get next bucket brigade  [500, #0]\". This is caused by by mod_deflate failing to check original file size. The check has to use modulo 2^32. Please see attached patch."}, {"attachment_id": null, "tags": [], "creator": "jkaluza@redhat.com", "text": "Fixed in trunk: revision 1572092", "count": 1, "id": 173493, "time": "2014-02-26T15:39:00Z", "bug_id": 56062, "creation_time": "2014-02-26T15:39:00Z", "is_private": false}, {"count": 2, "text": "FWIW you seem to have attributed the fix improperly; maybe you could have left the comment as it was.", "bug_id": 56062, "is_private": false, "id": 173750, "time": "2014-03-11T14:15:46Z", "creator": "lkundrak@v3.sk", "creation_time": "2014-03-11T14:15:46Z", "tags": [], "attachment_id": null}, {"count": 3, "tags": [], "creator": "ylavic.dev@gmail.com", "text": "Backported to upcoming 2.4.10.", "id": 175965, "time": "2014-06-21T22:51:21Z", "bug_id": 56062, "creation_time": "2014-06-21T22:51:21Z", "is_private": false, "attachment_id": null}, {"count": 4, "tags": [], "creator": "christophe.jaillet@wanadoo.fr", "attachment_id": null, "is_private": false, "id": 177221, "time": "2014-08-18T07:00:09Z", "bug_id": 56062, "creation_time": "2014-08-18T07:00:09Z", "text": "Fixed and released in 2.4.10"}]