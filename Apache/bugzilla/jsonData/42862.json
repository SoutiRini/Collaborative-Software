[{"count": 0, "tags": [], "creator": "jann.forrer@id.unizh.ch", "text": "If I add a new user the password field has already an entry", "id": 105307, "time": "2007-07-12T00:30:58Z", "bug_id": 42862, "creation_time": "2007-07-12T00:30:58Z", "is_private": false, "attachment_id": null}, {"count": 1, "tags": [], "text": "That's inserted by the browser. You can tell it not to remember passwords to\navoid it.\n\nThe problem is <input type=\"password\" name=\"password\"/>.\nWe could use e.g. <input type=\"password\" name=\"userPassword\"/>.", "attachment_id": null, "bug_id": 42862, "id": 105374, "time": "2007-07-13T06:48:47Z", "creator": "andreas@apache.org", "creation_time": "2007-07-13T06:48:47Z", "is_private": false}, {"count": 2, "tags": [], "creator": "andreas@apache.org", "text": "Fixed in revision 556004.", "id": 105375, "time": "2007-07-13T07:02:29Z", "bug_id": 42862, "creation_time": "2007-07-13T07:02:29Z", "is_private": false, "attachment_id": null}, {"count": 3, "tags": [], "creator": "thorsten@apache.org", "text": "Renaming Lenya 1.4 to 2.0", "id": 105646, "time": "2007-07-16T01:59:50Z", "bug_id": 42862, "creation_time": "2007-07-16T01:59:50Z", "is_private": false, "attachment_id": null}]